module Sem_1101_ValueVars_003 {
    type component GeneralComp {
        timer t;
        var integer vc_ai[3] := {1, 3, 5};
    }
    
	testcase TC_Sem_1101_ValueVars_003() runs on GeneralComp {
  		var integer v_i := 3;
        v_i := 5;
        vc_ai[1] := 5;
    
        if (vc_ai[1] == v_i){
            setverdict(pass);
        }
        else {
            setverdict(fail);
        }
	}
    
    control {
        execute(TC_Sem_1101_ValueVars_003());
    }
}