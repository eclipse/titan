module Sem_190301_select_case_statement_001 {
	
	type component GeneralComp {	
	}
	
	testcase TC_Sem_190301_select_case_statement_001 () runs on GeneralComp{
	 	var integer v_i:=2;
	 
	 	select (v_i) {	
	  	 case(1) {
	   		setverdict(fail);
	  	 }
	  	 case(2) {
	   		setverdict(pass);
	  	 }
	  	 case(3) {
	   		setverdict(fail);
	  	 }
	  	 case else {
	   		setverdict(fail);
	  	 }
	 	}
	}


	control{

		execute(TC_Sem_190301_select_case_statement_001());

	}

}
