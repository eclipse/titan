module Syn_26_ModuleControl_005 {

	type component GeneralComp {}

	testcase TC_Syn_26_ModuleControl_005() runs on GeneralComp 
	{ setverdict(pass);	}

	control{
		var integer v_i:=0;
		while (v_i < 10) {
			v_i := v_i + 1;
		}

		execute(TC_Syn_26_ModuleControl_005()); //not part of the test
	}

}
