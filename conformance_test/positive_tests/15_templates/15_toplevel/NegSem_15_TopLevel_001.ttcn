module NegSem_15_TopLevel_001 {

	type record MyRecord {
		default def
	}

	type union MyUnion {
		integer choice1,
		MyRecord choice2
	}

	template MyUnion m_integerChosen := {
		choice1 := 5
	}

	type component GeneralComp {}

	testcase TC_NegSem_15_TopLevel_001() runs on GeneralComp 
	{ setverdict(fail);	}
	
	control
	{ execute(TC_NegSem_15_TopLevel_001()); }

}
